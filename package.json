  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:win": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:auth": "jest --testPathPattern=auth.test.ts",
    "test:middleware": "jest --testPathPattern=middleware.test.ts",
    "test:services": "jest --testPathPattern=services.test.ts",
    "test:integration": "jest --testPathPattern=integration.test.ts",
    "test:trips": "jest --testPathPattern=trips.test.ts",
    "test:organizations": "jest --testPathPattern=organizations.test.ts",
    "test:activities": "jest --testPathPattern=activities.test.ts",
    "test:database": "jest --testPathPattern=database.test.ts",
    "test:unit": "jest --testPathIgnorePatterns=integration.test.ts --passWithNoTests",
    "test:e2e": "jest --testPathPattern=integration.test.ts",
    "test:security": "jest --testPathPattern='(auth|middleware)' --passWithNoTests",
    "test:performance": "jest --testTimeout=60000 --testPathPattern=performance",
    "test:verbose": "cross-env VERBOSE_TESTS=true SHOW_TEST_ERRORS=true jest --verbose",
    "test:silent": "jest --silent --passWithNoTests",
    "coverage:open": "npm run test:coverage && open coverage/lcov-report/index.html",
    "coverage:serve": "npm run test:coverage && npx http-server coverage/lcov-report -p 3001",
    "db:push": "drizzle-kit push",
    "db:seed": "cross-env NODE_ENV=development tsx scripts/seed-demo-data.ts",
    "seed:demo": "cross-env NODE_ENV=development tsx scripts/seed-demo-data.ts",
    "db:create-superadmin-tables": "cross-env NODE_ENV=development tsx scripts/create-superadmin-tables.ts",
    "db:seed-flags": "cross-env NODE_ENV=development tsx scripts/seed-feature-flags.ts"
  },

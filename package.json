  "scripts": {
    "build": "pnpm -r --filter=@shared/schema build && pnpm -r --filter=@server/api build",
    "build:shared": "pnpm --filter @shared/schema build",
    "build:server": "pnpm --filter @server/api build",
    "dev": "pnpm dev:server",
    "dev:server": "pnpm --filter @server/api dev:watch",
    "start": "pnpm --filter @server/api start",
    "clean": "node scripts/clean.mjs",
    "clean:directories": "node scripts/clean.mjs all",
    "clean:shared": "node scripts/clean.mjs shared",
    "clean:server": "node scripts/clean.mjs server",
    "typecheck": "pnpm -r --parallel typecheck",
    "typecheck:shared": "pnpm --filter @shared/schema typecheck",
    "typecheck:server": "pnpm --filter @server/api typecheck",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "db:push": "drizzle-kit push"
  },

# Client-side environment variables (used by Vite)
VITE_SUPABASE_URL="YOUR_SUPABASE_PROJECT_URL"
VITE_SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"

# Supabase Edge Function environment variables (set as Supabase Secrets)
# These are NOT directly read by the client-side application.
# You will set these in your Supabase project settings under 'Edge Functions' -> 'Secrets'.
SUPABASE_URL="YOUR_SUPABASE_PROJECT_URL"
SUPABASE_ANON_KEY="YOUR_SUPABASE_ANON_KEY"
SUPABASE_SERVICE_ROLE_KEY="YOUR_SUPABASE_SERVICE_ROLE_KEY" # Found in Supabase Project Settings -> API Keys
OPENAI_API_KEY="YOUR_OPENAI_API_KEY"
STRIPE_API_KEY="YOUR_STRIPE_SECRET_KEY" # e.g., sk_test_...
STRIPE_WEBHOOK_SIGNING_SECRET="YOUR_STRIPE_WEBHOOK_SIGNING_SECRET"
